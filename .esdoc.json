{
  "source": "./Reconstructive",
  "destination": "./docs",
  "plugins": [
    {"name": "esdoc-external-webapi-plugin", "option": {"enable": true}},
    {"name": "esdoc-external-ecmascript-plugin", "option": {"enable": true}},
    {
      "name": "esdoc-standard-plugin",
      "option": {
        "typeInference": {"enable": true},
        "undocumentIdentifier": {"enable": true},
        "unexportedIdentifier": {"enable": true},
        "brand": {
          "logo": "./resources/reconstructive-logo.png",
          "title": "Reconstructive: An Archival Replay ServiceWorker",
          "description": "Reconstructive is a ServiceWorker Module for Client-side Reconstruction of Composite Mementos",
          "repository": "https://github.com/oduwsdl/Reconstructive",
          "site": "https://oduwsdl.github.io/Reconstructive/",
          "author": "https://twitter.com/ibnesayeed",
          "image": "./resources/reconstructive-logo.png"
        }
      }
    }
  ]
}
